						{
							"name": "Login",
							"event": [
								{
									"listen": "test",
									"script": {
										"exec": [
											"// Récupère les cookies de la réponse\r",
											"var cookies = pm.cookies;\r",
											"\r",
											"// Vérifie que le cookie 'refreshToken' est présent\r",
											"if (cookies.has('refreshToken')) {\r",
											"    var refreshToken = cookies.get('refreshToken');\r",
											"    pm.environment.set('refreshToken', refreshToken);\r",
											"} else {\r",
											"    console.error('Erreur : Le cookie refreshToken n\\'est pas présent dans la réponse.');\r",
											"}\r",
											"\r",
											"// Récupère l'accessToken de la réponse JSON\r",
											"var response = pm.response.json();\r",
											"if (response.accessToken) {\r",
											"    pm.environment.set('authToken', response.accessToken);\r",
											"} else {\r",
											"    console.error('Erreur : accessToken n\\'est pas présent dans la réponse.');\r",
											"}\r",
											""
										],
										"type": "text/javascript",
										"packages": {}
									}
								}
							],
							"request": {
								"method": "POST",
								"header": [],
								"body": {
									"mode": "raw",
									"raw": "{\n    \"username\": \"Arthur Kameni\",\n    \"email\": \"arthur.kamenitchualeu@gmail.com\"\n}",
